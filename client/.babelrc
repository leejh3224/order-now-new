{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "components": "./src/components",
        "screens": "./src/screens",
        "config": "./src/config",
        "containers": "./src/containers",
        "store": "./src/store",
        "utils": "./src/utils"
      }
    }]
  ]
}